enum Permission {
  ADMIN
  USER
  NOTEUPDATE
  NOTEDELETE
  PERMISSIONUPDATE
  GAMECREATE
  BAN
}

type User {
  id: ID! @unique
  username: String! @unique
  email: String! @unique
  password: String!
  resetToken: String
  resetTokenExpiry: Float
  permissions: [Permission]
}

type Game {
  id: ID! @unique
  name: String! @unique
  characters: [String]!
  filters: [String]!
}

type GlobalFilter {
  id: ID! @unique
  name: String! @unique
}